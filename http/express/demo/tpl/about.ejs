<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>EJS Demo</title>
</head>
<body>
  <div>
    <% include ./header %>
  </div>

  <div>
    <h2> <%= title %> </h2>
    <ul>
      <% info.forEach(function(info){ %>
        <li><strong>Name:</strong><%= info.name %>  --- <strong>age:</strong><%= info.age %></li>

      <% }); %>
    </ul>
  </div>

  <div>
    <% include ./footer %>
  </div>

</body>
</html>
